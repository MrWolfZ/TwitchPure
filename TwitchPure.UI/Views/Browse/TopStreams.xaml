<Page x:Class="TwitchPure.UI.Views.Browse.TopStreams"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:prismMvvm="using:Prism.Windows.Mvvm"
      xmlns:viewModels="using:TwitchPure.UI.ViewModels.Browse"
      xmlns:controls="using:TwitchPure.UI.Controls"
      mc:Ignorable="d"
      d:DataContext="{d:DesignInstance viewModels:TopStreamsViewModel}"
      prismMvvm:ViewModelLocator.AutoWireViewModel="True">

  <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
    <controls:Navbar DataContext="{Binding NavbarViewModel}">
      <TextBlock Text="Top Streams" Margin="12,12,0,0" Style="{StaticResource BaseTextBlockStyle}" />
    </controls:Navbar>
  </Grid>
</Page>